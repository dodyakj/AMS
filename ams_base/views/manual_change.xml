<odoo>
	<data>
    <record id="ams_manual_changes_form" model="ir.ui.view">
        <field name="name">ams.manual_changes.form</field>
        <field name="model">ams.manual_changes</field>
        <field name="view_mode">tree,form</field>
        <field name="arch" type="xml">
            <form string="Manual Changes">
            	<group col="4">
                    <field name="fleet_id"/>
					<field name="engine_id"/>
					<field name="auxiliary_id"/>
					<field name="propeller_id"/>
            	</group>
            	<group col="4">
            		<field name="current_hours"/>
            		<field name="hours"/>
            		<field name="current_cycles"/>
            		<field name="cycles"/>
            		<field name="current_rin"/>
            		<field name="rin"/>
            	</group>
            	<group>
					<field name="timestamp"/>
					<field name="employee"/>
            	</group>
            </form>
        </field>
    </record>
	</data>
</odoo>
