<odoo>
  <data>
    <record model='ir.ui.view' id='receiving_template_tree'>
      <field name='name'>receiving.template.tree</field>
      <field name='model'>receiving.template</field>
      <field name='arch' type='xml'>
        <tree string='Receiving Inventory Template'>
          <field name='name'/>
        </tree>
      </field>
    </record>

    <record model="ir.ui.view" id="receiving_template_form">
      <field name="name">receiving.template.form</field>
      <field name="model">receiving.template</field>
      <field name="arch" type="xml">
        <form string="Receiving Inventory Template">
          <sheet>
            <group>
              <field name='name'/>
            </group>
          </sheet>
        </form>
      </field>
    </record>

    <record id="receiving_template_action" model="ir.actions.act_window">
      <field name="name">Receiving Inventory Template</field>
      <field name="res_model">receiving.template</field>
      <field name="view_type">form</field>
      <field name="view_mode">tree,form</field>
    </record>
  </data>
</odoo>